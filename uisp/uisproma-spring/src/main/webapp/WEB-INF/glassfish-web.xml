<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD 
GlassFish Application Server 3.1 Servlet 3.0//EN" 
"http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app>
	<context-root>/sportroma</context-root>
	
	<security-role-mapping>
		<role-name>ADMINISTRATOR</role-name>
		<group-name>ADMINISTRATOR</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>CURATOR</role-name>
		<group-name>CURATOR</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>ATTACHE</role-name>
		<group-name>ATTACHE</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>REFEREE</role-name>
		<group-name>REFEREE</group-name>
	</security-role-mapping>
	
	<class-loader delegate="true" />
	
	<jsp-config>
		<property name="keepgenerated" value="true">
			<description>Keep a copy of the generated servlet class java code.</description>
		</property>
	</jsp-config>

</glassfish-web-app>