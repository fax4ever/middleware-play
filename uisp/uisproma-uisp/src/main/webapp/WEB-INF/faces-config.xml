<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
	<application>
		<locale-config>
            <default-locale>it</default-locale>
            <supported-locale>it</supported-locale>
        </locale-config>
        <message-bundle>
	  		com.whiterational.uisproma.application
	  	</message-bundle>
	  	<resource-bundle>
			<base-name>com.whiterational.uisproma.application</base-name>
			<var>msg</var>
	   	</resource-bundle>
	   	<resource-bundle>
	   		<base-name>com.whiterational.uisproma.presentation.faces.config</base-name>
	   		<var>config</var>
	   	</resource-bundle>
	   	<resource-bundle>
	   		<base-name>uisp</base-name>
	   		<var>uisp</var>
	   	</resource-bundle>
	</application>

</faces-config>
